#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 10
#define ROW 10
#define COL 3

void array_print(int *A, int size){
	printf("A[] = { ");
	for (int i = 0; i < size; i++) {
		printf("%d", A[i]);
		if (i < size-1) {
			printf(", ");
		}
	}
	printf(" }");
}

int main(void) {
	srand((unsigned)time(NULL));
	int arr[SIZE] = { 1,2,3,4,5 };
	array_print(arr, SIZE);
	

}
