#include <stdio.h>
#include <math.h>

double get_distance(double x1, double y1, double x2, double y2) {
	double distance, distance_x, distance_y;
	distance_x = x1 - x2;
	distance_y = y1 - y2;
	distance = sqrt(distance_x * distance_x + distance_y * distance_y);

	return distance;
}
int main(void) {
	int x1,y1,x2,y2;
	scanf_s("%d %d", &x1, &y1);
	scanf_s("%d %d", &x2, &y2);
	
	printf("%lf", get_distance(x1,y1,x2,y2));
	

}
