#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int binomial_coefficient(int n, int k) {
	if (k == 0 || k == n) {
		return 1;
	}
	else {
		return binomial_coefficient(n - 1, k - 1) + binomial_coefficient(n - 1, k);
	}
		
}

int main(void) {
	
	int n, k;
	printf("n: ");
	scanf_s("%d", &n);
	printf("k: ");
	scanf_s("%d", &k);
	printf("%d", binomial_coefficient(n, k));
}
