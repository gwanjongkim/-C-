#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 10


int main(void) {
	srand((unsigned)time(NULL));
	int rand_num[SIZE] = { 0 };

	for (int i = 0; i < 100; i++) {
		rand_num[rand() % 10] += 1;
	}
	int max = rand_num[0];
	for (int i = 0; i < SIZE; i++) {
		if(rand_num[i] > max){
			max = rand_num[i];
		}
	}

	for (int i = 0; i < SIZE; i++) {
		if (rand_num[i] == max) {
			printf("max num is %d", i + 1);
		}
	}
}
