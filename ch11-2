#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 10
#define ROW 10
#define COL 3

void getget_sum_diff(int x, int y, int* p_sum, int* p_diff) {
	*p_sum = x + y;
	*p_diff = x - y;
}

int main(void) {
	int sum, diff;
	getget_sum_diff(3, 5, &sum, &diff);
	printf("%d %d", sum, diff);

}
