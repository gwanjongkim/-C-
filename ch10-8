#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 10
#define ROW 10
#define COL 3


int main(void) {
	int arr[ROW][COL];
	
	for (int i = 0; i < ROW; i++) {
		
		arr[i][0] = i + 1;
		arr[i][1] = (i + 1) * (i + 1);
		arr[i][2] = (i + 1) * (i + 1) * (i + 1);
	}

	int cube;
	printf("input cube value: ");
	scanf_s("%d", &cube);

	for (int i = 0; i < ROW; i++) {
		if (arr[i][2] == cube) {
			printf("cube root is %d", arr[i][0]);
		}
	}
}
