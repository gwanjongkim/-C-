#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main(void) {
	srand((unsigned)time(NULL));
	int x[10];
	for (int i = 0; i < 10; i++) {
		x[i] = rand();
	}
	
	for (int i = 0; i < 9; i++) {
		int temp;
		
		for (int j = i+1; j < 10; j++) {
			if (x[i] > x[j]) {
				temp = x[i];
				x[i] = x[j];
				x[j] = temp;
			}
		}
	}
	printf("max is %d, min is %d", x[9], x[0]);
}
