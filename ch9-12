#include <stdio.h>
#include <stdlib.h>
#include <time.h>


int fib(int n) {
	if (n == 0) {
		
		return 0;
	}
	else if (n == 1) {
		
		return 1;
	}
	else {
		
		return fib(n - 1) + fib(n - 2);
	}
}

int main(void) {
	
	int n = 10;
	for (int i = 0; i < 10; i++) {
		printf("fib(%d) = %d\n", i, fib(i));
	}
	
}
