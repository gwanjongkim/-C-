#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 10
#define ROW 10
#define COL 3

void scalar_multi(int a[][3], int scalar) {
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++) {
			a[i][j] = a[i][j] * scalar;
			printf("%d ", a[i][j]);
		}
		printf("\n");
	}
}

void transpose(int a[][3], int b[][3]) {
	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++) {
			b[j][i] = a[i][j];
		}
	}

	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 3; j++) {
			b[i][j];
			printf("%d ", b[i][j]);
		}
		printf("\n");
	}
}

int main(void) {
	int arr[3][3] = { 1,2,3,4,5,6,7,8,9 };
	int arr2[3][3];
	
	transpose(arr, arr2);
	scalar_multi(arr, 2);
	
	
}
