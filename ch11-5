#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define SIZE 10
#define ROW 10
#define COL 3

void convert(double* grades, double* scores, int size) {
	for (int i = 0; i < size; i++) {
		scores[i] = grades[i] / 4.5 * 100;
	}
}

int main(void) {
	srand((unsigned)time(NULL));
	double grades[SIZE];
	double score[SIZE];

	for (int i = 0; i < SIZE; i++) {
		grades[i] = 0.5 * i;
	}

	convert(grades, score, SIZE);
	for (int i = 0; i < SIZE; i++) {
		printf("%lf ", score[i]);
	}

	

}
